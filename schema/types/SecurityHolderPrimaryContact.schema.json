      "description": "担当者名",
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/Name.schema.json"
    },
    "phone_numbers": {
      "description": "担当者の電話番号",
      "type": "array",
      "items": {
        "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/Phone.schema.json"
      }
    },
    "emails": {
      "description": "担当者のメールアドレス",
      "type": "array",
      "items": {
        "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/Email.schema.json"
      }
